{{page-title (t "general.admin")}}
<LinkTo @route="admin-dashboard">
  {{t "general.backToAdminDashboard"}}
</LinkTo>
<PendingUserUpdates
  @filter={{this.filter}}
  @limit={{this.limit}}
  @offset={{this.offset}}
  @schools={{this.model.schools}}
  @primarySchool={{this.model.primarySchool}}
  @school={{this.schoolId}}
  @setSchool={{set this.school}}
  @setFilter={{set this.filter}}
/>
