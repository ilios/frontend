{{title (t "general.admin")}}
<LinkTo @route="admin-dashboard">
  {{t "general.backToAdminDashboard"}}
</LinkTo>
<IliosUsers
  @offset={{this.offset}}
  @setOffset={{action (mut this.offset)}}
  @limit={{this.limit}}
  @setLimit={{action (mut this.limit)}}
  @query={{this.query}}
  @setQuery={{action (mut this.query) value="target.value"}}
  @showNewUserForm={{this.showNewUserForm}}
  @setShowNewUserForm={{action (mut this.showNewUserForm)}}
  @showBulkNewUserForm={{this.showBulkNewUserForm}}
  @setShowBulkNewUserForm={{action (mut this.showBulkNewUserForm)}}
  @searchTerms={{this.searchTerms}}
  @setSearchTerms={{action (mut this.searchTerms)}}
  @transitionToUser={{action "transitionToUser"}}
/>
