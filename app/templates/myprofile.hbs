{{page-title (t "general.myProfile")}}
<MyProfile
  @user={{@model}}
  @showCreateNewToken={{this.showCreateNewToken}}
  @toggleShowCreateNewToken={{this.toggleShowCreateNewToken}}
  @showInvalidateTokens={{this.showInvalidateTokens}}
  @toggleShowInvalidateTokens={{this.toggleShowInvalidateTokens}}
  @permissionsSchool={{this.permissionsSchool}}
  @permissionsYear={{this.permissionsYear}}
  @setPermissionsSchool={{set this.permissionsSchool}}
  @setPermissionsYear={{set this.permissionsYear}}
/>
