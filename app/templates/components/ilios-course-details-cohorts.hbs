<table class='grid-table'>
  <thead>
    <tr>
      <th>{{t 'general.program'}}</th>
      <th>{{t 'general.cohort'}}</th>
      <th>{{t 'general.members'}}</th>
      <th>{{t 'general.level'}}</th>
    </tr>
  </thead>
  <tbody>
    {{#each cohort in cohorts}}
      <tr>
        <td>{{cohort.programYear.program.title}}</td>
        <td>{{cohort.displayTitle}}</td>
        <td></td>
        <td></td>
      </tr>
    {{/each}}
  </tbody>
</table>
