<div class="curriculum-inventory-verification-preview-table3a" data-test-curriculum-inventory-verification-preview-table3a ...attributes>
  <h4 data-test-title id="verification-preview-table3a">
    {{t "general.table3aNonClerkshipSequenceBlockInstructionalTime"}}
  </h4>
  <table>
    <thead>
      <tr>
        <th colspan="2">
          {{t "general.nonClerkshipSequenceBlocks"}}
        </th>
        <th>
          {{t "general.academicLevel"}}
        </th>
        <th>
          {{t "general.totalWeeks"}}
        </th>
        <th>
          {{t "general.averageHoursOfInstructionPerWeek"}}
        </th>
      </tr>
    </thead>
    <tbody>
      {{#each @data as |row|}}
        <tr>
          <td colspan="2">
            {{row.title}}
          </td>
          <td>
            {{row.level}}
          </td>
          <td>
            {{row.weeks}}
          </td>
          <td>
            {{row.avg}}
          </td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>
