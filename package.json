{
  "name": "lti-app",
  "version": "12.0.1",
  "description": "LTI App for ilios integration",
  "repository": "https://github.com/ilios/lti-app",
  "license": "MIT",
  "author": "The Ilios team <info@iliosproject.org>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint app/templates",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "lint:style": "stylelint app/styles",
    "deploy:production": "ember deploy production --activate",
    "deploy:staging": "ember deploy staging --activate"
  },
  "devDependencies": {
    "@csstools/postcss-sass": "^4.0.0",
    "@ember/edition-utils": "^1.2.0",
    "@ember/jquery": "^1.1.0",
    "@ember/optional-features": "^2.0.0",
    "@glimmer/component": "^1.0.2",
    "@glimmer/tracking": "^1.0.2",
    "@ilios/eslint-config-ember": "^2.4.0",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "broccoli-file-creator": "^2.1.1",
    "broccoli-merge-trees": "^4.2.0",
    "browserslist": "^4.14.5",
    "caniuse-db": "^1.0.30001156",
    "ember-ajax": "^5.0.0",
    "ember-auto-import": "^1.6.0",
    "ember-cli": "~3.21.2",
    "ember-cli-app-version": "^3.2.0",
    "ember-cli-babel": "^7.22.1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-dependency-lint": "2.0.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-aws-pack": "^1.0.1",
    "ember-cli-htmlbars": "^5.3.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-postcss": "^7.0.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.0",
    "ember-export-application-global": "^2.0.1",
    "ember-load": "^0.0.17",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.2",
    "ember-simple-set-helper": "^0.1.2",
    "ember-source": "3.20.3",
    "ember-template-lint": "^2.12.1",
    "ember-web-app": "^5.0.0",
    "eslint": "^7.12.1",
    "ilios-common": "^51.2.0",
    "loader.js": "^4.7.0",
    "normalize.css": "8.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-scss": "^3.0.1",
    "qunit-dom": "^1.4.0",
    "stylelint": "^13.7.2",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.17.2"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "paths": [
      "lib/ilios-error",
      "lib/ilios-loading"
    ]
  },
  "pre-commit": [
    "lint:hbs",
    "lint:style",
    "lint:js"
  ],
  "private": true
}
