<div class="backtolink backtosessionlink" data-test-back-to-sessions>
  {{! template-lint-disable no-implicit-this }}
  <LinkTo @route="course" @model={{await @model.course}} @query={{hash}}>
    {{t "general.backToSessionList"}}
  </LinkTo>
</div>
<SessionDetails
  @session={{@model}}
  @editable={{this.canUpdate}}
  @programs={{await @model.course}}
  @sessionTypes={{await @model.course.school.sessionTypes}}
  @sessionObjectiveDetails={{this.sessionObjectiveDetails}}
  @sessionTaxonomyDetails={{this.sessionTaxonomyDetails}}
  @setSessionObjectiveDetails={{set this.sessionObjectiveDetails}}
  @setSessionTaxonomyDetails={{set this.sessionTaxonomyDetails}}
  @isManagingLearnerGroups={{this.isManagingLearnerGroups}}
  @setIsManagingLearnerGroups={{set this.isManagingLearnerGroups}}
  @sessionLearnergroupDetails={{this.sessionLearnergroupDetails}}
  @setSessionLearnergroupDetails={{set this.sessionLearnergroupDetails}}
  @showNewOfferingForm={{this.addOffering}}
  @toggleShowNewOfferingForm={{fn (set this.addOffering) (not this.addOffering)}}
  @sessionLeadershipDetails={{this.sessionLeadershipDetails}}
  @setSessionLeadershipDetails={{set this.sessionLeadershipDetails}}
  @sessionManageLeadership={{this.sessionManageLeadership}}
  @setSessionManageLeadership={{set this.sessionManageLeadership}}
/>
